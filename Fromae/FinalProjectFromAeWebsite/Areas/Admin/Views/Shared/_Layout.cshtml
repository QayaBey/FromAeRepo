
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>Dashio - Bootstrap Admin Template</title>

    <!-- Favicons -->
    <link href="~/admin/img/favicon.png" rel="icon">
    <link href="~/admin/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="~/admin/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="~/admin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/admin/css/zabuto_calendar.css">
    <link rel="stylesheet" type="text/css" href="~/admin/lib/gritter/css/jquery.gritter.css" />
    <!-- Custom styles for this template -->
    <link href="~/admin/css/style.css" rel="stylesheet">
    <link href="~/admin/css/style-responsive.css" rel="stylesheet">
    <script src="~/admin/lib/chart-master/Chart.js"></script>
</head>

<body>
    <section id="container" class="container">
        <!-- **********************************************************************************************************************************************************
            TOP BAR CONTENT & NOTIFICATIONS
            *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a href="index.html" class="logo"><b>DASH<span>IO</span></b></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                </ul>
                <!--  notification end -->
            </div>
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="login.html">Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
            MAIN SIDEBAR MENU
            *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <div class="row">
            <aside class="col-3">
                <div id="sidebar" class="nav-collapse ">
                    <!-- sidebar menu start-->
                    <ul class="sidebar-menu" id="nav-accordion">
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Menus" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Menu</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="SubMenus" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>SubMenu</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Images" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Image</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Products" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Product</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Categories" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Category</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="ProductCategories" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>ProductCategory</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="ProductProperties" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>ProductProperty</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Properties" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Property</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Ratings" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Rating</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Comments" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Comment</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Models" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Model</span>
                            </a>
                        </li>
                        <li class="">
                            <a class="active" asp-action="Index" asp-controller="Markas" asp-area="Admin">
                                <i class="fa fa-dashboard"></i>
                                <span>Marka</span>
                            </a>
                        </li>

                    </ul>
                    <!-- sidebar menu end-->
                </div>
            </aside>
            <div class="col-9" style="margin-left:240px; margin-top:80px;">@RenderBody()</div>
        </div>
    </section>
</body>

</html>

